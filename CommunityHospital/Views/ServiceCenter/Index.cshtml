@model IEnumerable<CommunityHospital.Models.ServiceCenter>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ADistrict.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AThana.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ADistrict.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AThana.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ServiceCenterId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ServiceCenterId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ServiceCenterId })
        </td>
    </tr>
}

</table>
